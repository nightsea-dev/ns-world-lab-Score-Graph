{
    // @ns-sg/web
    // tsconfig.json
    "compilerOptions": {
        "target": "ES2020",
        "lib": [
            "DOM",
            "DOM.Iterable",
            "ES2020"
        ],
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "jsx": "react-jsx",
        "strict": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "useDefineForClassFields": true,
        "isolatedModules": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "noEmit": true,
        "types": [
            "vite/client"
        ],
        "baseUrl": ".",
        "paths": {
            "@ns-sg/types": [
                "../../packages/types/src/index.ts"
            ],
            "@ns-sg/types/*": [
                "../../packages/types/src/*"
            ]
        },
    },
    "include": [],
    "exclude": [],
    "references": [
        {
            "path": "./tsconfig.app.json"
        }
    ]
}